<?xml version="1.0"?>

<Form Caption="Waypoint Editor">
  <CloseButton X="5" Y="-37" Width="80" Height="35"/>

  <Edit Name="Name" Caption="Name" Y="0" Height="28" CaptionWidth="75">
    <DataField DataType="string"/>
  </Edit>

  <Edit Name="Comment" Caption="Comment" Height="28" CaptionWidth="75">
    <DataField DataType="string"/>
  </Edit>

  <Edit Name="Location" Caption="Location" Height="28" CaptionWidth="75">
    <DataField DataType="geopoint"/>
  </Edit>

  <Edit Name="prpAltitude" Caption="Altitude" Height="28" CaptionWidth="90">
    <DataField DataType="double" DisplayFormat="%.0f %s" EditFormat="%.0f" Min="0" Max="30000" Step="5"/>
  </Edit>

  <Edit Name="prpFlags" Caption="Type" Height="28" CaptionWidth="90">
    <DataField DataType="enum" />
  </Edit>
</Form>
